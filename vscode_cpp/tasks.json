{
    "tasks": [
        {
            "type": "shell",
            "label": "C/C++: cmake generate mingw configure file",
            "command": "cmake",
            "args": [
                "-S",
                "${workspaceFolder}\\",
                "-B",
                "${workspaceFolder}\\build\\",
                "-G",
                "MinGW Makefiles",
                "-DCMAKE_BUILD_TYPE=Debug"
            ],
            "options": {
                "cwd": "${workspaceFolder}"
            },
            "problemMatcher": [
                "$gcc"
            ],
            "group": {
                "kind": "none"
            },
            "presentation": {
                "echo": true,
                "reveal": "always",
                "revealProblems": "onProblem",
                "focus": false,
                "panel": "shared",
                "showReuseMessage": true,
                "clear": false,
                "close": false
            },
            "detail": "Task generated by Debugger."
        },
